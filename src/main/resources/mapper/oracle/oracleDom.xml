<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.dao.domeDao">
    <insert id="addUser">
        insert into user2 values (#{id},#{name},#{password})
    </insert>


    <select id="getUser" resultType="com.example.entity.User">
        select * from user2
    </select>
    <select id="demandID" resultType="java.lang.Integer">
        select ID from user2
    </select>
</mapper>